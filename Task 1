package com.company;
import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.println("Введите число:");
        int a = scan.nextInt();
        scan.close();
        if (a > 7) {
            System.out.println("привет");
        }

    }
}
